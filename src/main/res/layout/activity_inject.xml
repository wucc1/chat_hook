<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:gravity="center" android:layout_width="match_parent" android:layout_height="wrap_content">
        <Button android:id="@+id/btn_inject" android:padding="3dp" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/show_sn_str"/>
    </LinearLayout>
    <ScrollView android:paddingLeft="5dp" android:paddingRight="5dp" android:layout_width="match_parent" android:layout_height="wrap_content">
        <TextView android:id="@+id/text_word" android:paddingLeft="5dp" android:paddingRight="5dp" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/hello_world"/>
    </ScrollView>
</LinearLayout>
